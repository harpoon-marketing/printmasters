        "button": "0.5rem"
      },
      "padding": {
        "inline": "clamp(1rem, -1.1429rem + 5.7143vw, 4rem)",
        "block": "clamp(1.5rem, 8.75vw + -1.781rem, 5rem)"
      },
      "margin": {
        "inline": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
        "block": "clamp(1.5rem, 8.75vw + -1.781rem, 5rem)"
      },
      "transition": "all 0.3s cubic-bezier(0.12, 0.72, 0.33, 1)"
