        "heading-1": "clamp(2.75rem, 0.6vw + 2.525rem, 3.125rem)"
      },
      "gap": {
        "row": "1.5rem",
        "inline": "clamp(1rem, 1.25vw + 0.531rem, 1.5rem)",
