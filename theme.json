        {
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          },
          "name": "Heading 4",
          "size": "1.5rem",
          "slug": "Heading-4"
        },
        {
          "fluid": {
            "min": "1.5rem",
            "max": "1.875rem"
          },
          "name": "Heading 3",
          "size": "1.875rem",
          "slug": "Heading-3"
        },
        {
          "fluid": {
            "min": "1.875rem",
