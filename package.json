{
  "name": "wagada-press",
  "version": "0.3.0",
  "description": "wordpress theme",
  "author": "Wagada Digital",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "styles:app": "npx sass -w --style=compressed src/app.scss resources/styles/app.css --no-source-map",
    "styles:editor": "npx sass -w --style=compressed src/editor.scss resources/styles/editor.css --no-source-map",
    "start": "concurrently \"npm:styles:*\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.77.8"
  }
}
